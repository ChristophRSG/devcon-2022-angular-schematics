{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "TemplateExampleSchema",
  "title": "Schema for template-example",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "default": "noName"
    },
    "magicNumber": {
      "type": "number",
      "_x-prompt": "what is your magic number?"
    }
  },
  "required": [
    "magicNumber"
  ]
}
